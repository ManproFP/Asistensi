/*Style dasar untuk tombol secara umum pada panel kanan Home.php (Khusus untuk halaman koordinator)*/
.note_option{
	float:right;
	margin-top:-2px;
	margin-left:2px;
	padding-left:3px;
	padding-right:3px;
	text-align:center;
}

/*Stye untuk hover tombol secara umum pada panel kanan Home.php (Khusus untuk halaman koordinator)*/
.note_option:hover{
	cursor:pointer;
}

/*Style dasar untuk tombol DELETE pada panel kanan Home.php (Khusus untuk halaman koordinator)*/
.note_option:first-child{
	border: 1px solid #444;
	border-radius:3px;
	background: linear-gradient(to bottom, #ddd, #444);
	color:red;
	height:16px;
	line-height:16px;
	vertical-align:middle;
	font-weight:bold;
	font-size:12px;
	width:8px;
}

/*Style untuk hover tombol DELETE pada panel kanan Home.php (Khusus untuk halaman koordinator)*/
.note_option:first-child:hover{
	background: linear-gradient(to bottom, red, black);
	color:white;
}

/*Style dasar untuk tombol EDIT pada panel kanan Home.php (Khusus untuk halaman koordinator)*/
.note_option:nth-child(2){
	border: 1px solid #444;
	border-radius:3px;
	background: linear-gradient(to bottom, #ddd, #444);
	color:#5D1B2B;
	height:16px;
	line-height:16px;
	vertical-align:middle;
	font-weight:bold;
	font-size:11px;
	width:25px;
}

/*Style untuk hover pada tombol EDIT pada panel kanan Home.php (Khusus untuk halaman koordinator)*/
.note_option:nth-child(2):hover{
	background: linear-gradient(to bottom, #5D1B2B, #5E1B2C);
	color:#ddd;
}

.note_title{
	margin-top:15px;
	text-align:left;
	color:#5D1B2B;
	font-size:14px;
	font-weight:bold;
}

/*Style untuk pop up*/
.pop_up{
	display:none;
	position:fixed;
	left:0;
	top:0;
	background-color: rgba(0, 0, 0, 0.85);
	min-width: 800px;
	width: 100%;
	min-height: 500px;
	height: 100%;
	z-index: 10;
}

.popup_content{
	display:none;
	border: 0px none;
    min-width: 350px;
    width: 92%;
    min-height: 300px;
	margin-right:15px;
    float: right;
    margin-top: 20px;
    font-family: "Times New Roman";
}

.popup_close_button{
	border-radius: 6px;
    width: 18px;
    height: 18px;
    background: linear-gradient(to bottom, #F00, #8B0000) repeat scroll 0% 0% transparent;
    border-width: 2px;
    border-style: none solid solid;
    border-color: #F00 #8B0000 #8B0000;
    color: #FFF;
    text-align: center;
    line-height: 20px;
    font-size: 15px;
    font-family: arial;
    vertical-align: middle;
    float: right;
    font-weight: bold;
    margin-top: -10px;
    margin-right: -10px;
}

.popup_close_button:hover{
	cursor:pointer;
}

.popup_wrapper{
    position: static;
    font-size: 15px;
    padding: 20px;
    margin: 100px auto;
    background-color: #FFF;
    min-width: 500px;
    width: 50%;
    min-height: 350px;
    text-align: center;
    border: 1px solid #5D1B2B;
    box-shadow: 0px 0px 35px 5px #5E1B2C inset;
    border-radius: 15px;
}

.profile_edit_data{
	margin-right:100px;
